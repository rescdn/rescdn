        <!--<table tableName="integral_history" domainObjectName="IntegralHistory" schema="general" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" > <property name="useActualColumnNames" value="true"/></table>-->
        <!--<table tableName="order" domainObjectName="Order" schema="general" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" > <property name="useActualColumnNames" value="true"/></table>-->
        <!--<table tableName="shopping_card" domainObjectName="ShoppingCard" schema="general" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" > <property name="useActualColumnNames" value="true"/></table>-->
